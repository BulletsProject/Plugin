{
	"manifest_version": 2,
	"name": "Bullets",
	"version": "0.1",
	"icons": {
		"128": "res/gfx/icon.png"
	},
	"browser_action": {
		"default_icon": "res/gfx/icon.png"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["js/content.js"]
		}
	],
	"permissions": [
		"tabs",
		"background",
		"<all_urls>"
	]
}