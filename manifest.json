{
	"manifest_version": 2,
	"name": "Bullets",
	"version": "0.1",
	"icons": {
		"16": "res/gfx/icon19.png",
		"128": "res/gfx/icon.png"
	},
	"browser_action": {
		"default_icon": "res/gfx/icon19.png"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["libs/jquery/jquery-2.1.0.min.js", "js/content.js"],
			"css": ["res/style/plugin.css"]
		}
	],
	"permissions": [
		"tabs",
		"background",
		"<all_urls>"
	],

	"web_accessible_resources": [
		"*"
	]
}